<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SecurityInAMobile.MenuPage">
    <ContentPage.Content>
        <Grid>
            <Button x:Name="Settings" Text="Settings" Grid.Column="0" Grid.Row="0"
                    HorizontalOptions="Start" VerticalOptions="Center" Clicked="Settings_Clicked"/>
            <Button x:Name="Help" Text="Settings" Grid.Column="0" Grid.Row="0"
                    HorizontalOptions="Start" VerticalOptions="Center" Clicked="Help_Clicked"/>
        </Grid>
    </ContentPage.Content>
</ContentPage>